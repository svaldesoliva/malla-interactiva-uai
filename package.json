{
  "name": "malla-interactiva-uai",
  "version": "2.0.0",
  "description": "Adaptación de la malla interactiva para la Universidad Adolfo Ibáñez.",
  "main": "index.html",
  "devDependencies": {
    "browser-sync": "^2.26.13",
    "csso-cli": "^3.0.0",
    "eslint": "^8.57.1",
    "terser": "^5.5.1"
  },
  "scripts": {
    "build": "node scripts/build.js --production",
    "build:dev": "node scripts/build.js",
    "build:legacy": "./scripts/minify_code.sh",
    "devBuild": "./scripts/minify_dev.sh",
    "gbpages": "./scripts/minify_GBPages.sh",
    "clean": "npm prune --production",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prestart": "npm run build:dev",
    "start": "browser-sync start --config bs-config.js",
    "dev": "npm run build:dev && npm start",
    "cleanDev": "./scripts/clean.sh",
    "lint": "eslint src/js/**/*.js",
    "lint:fix": "eslint src/js/**/*.js --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/svaldesoliva/malla-interactiva-uai.git"
